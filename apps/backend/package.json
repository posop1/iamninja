{
  "name": "backend",
  "version": "1.0.0",
  "main": "main.ts",
  "scripts": {
    "build": "tsc",
    "start": "node dist/main.js",
    "start:prod": "prisma generate && npm run start",
    "dev": "nodemon -L ./main.ts | npx pino-pretty",
    "migrate": "dotenv -e ./.env prisma migrate dev --name init && dotenv -e ./.env prisma migrate deploy",
    "migrate:dev": "dotenv -e ./.env.dev prisma migrate dev --name init && dotenv -e ./.env.dev prisma migrate deploy",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.21",
    "@types/node": "^20.5.0",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "eslint": "^8.47.0",
    "nodemon": "^3.0.1",
    "pino-pretty": "^10.2.3",
    "prisma": "^5.6.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@prisma/client": "^5.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "pino": "^8.16.2",
    "pino-http": "^9.0.0"
  }
}
